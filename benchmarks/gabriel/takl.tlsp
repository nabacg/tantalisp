(def listn (fn [n]
  (if (= n 0)
    '()
    (cons n (listn (- n 1))))))

(def shorterp (fn [x y]
  (if y
    (if x
      (shorterp (cdr x) (cdr y))
      :true)
    :false)))

(def mas (fn [x y z]
  (if (shorterp y x)
    (mas
      (mas (cdr x) y z)
      (mas (cdr y) z x)
      (mas (cdr z) x y))
    z)))

(def l18 (listn 18))
(def l12 (listn 12))
(def l6 (listn 6))

(mas l18 l12 l6)
